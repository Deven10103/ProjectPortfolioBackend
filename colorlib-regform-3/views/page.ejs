<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">

    <!-- Title Page-->
    <title>Au Register Forms by Colorlib</title>

    <!-- Icons font CSS-->
    <link href="vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <link href="vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Vendor CSS-->
    <link href="vendor/select2/select2.min.css" rel="stylesheet" media="all">
  

    <!-- Main CSS-->
    <link href="css/main.css" rel="stylesheet" media="all">
</head>

<body>
   
    <div class="page-wrapper mainbg p-t-180 p-b-100 font-poppins">
        <div class="wrapper wrapper--w780">
            <div class="card card-3">
                <div class="card-heading"></div>
                <div class="card-body">
                   <div>
                    <div>Add Photos</div>
                    <div>Add Event</div>
                   </div>
                   <div class="form-1">
                    <form action="/upload7053" method="POST">
                        <div class="fileHolder">
                            <div>
                               
                                <label class="upload" for="fileButton">
                                    <input type="file" id="fileButton" name="file" >
                                    <!-- <i class="fa-solid fa-upload"></i> -->
                                    Upload</label>
                            </div>
                            <div class="input-group">
                                <input class="input--style-3" type="text" placeholder="file-name" name="name">
                            </div>
                        </div>
                        <!-- <span class="select2-selection__rendered" id="select2-event-wh-container" title="Event">Event</span> -->
                        <!-- <span class="select2-selection__rendered" id="select2-event-tt-container" title="Fres">Fres</span> -->
                       
                        <div class="input-group">
                            <div class="rs-select2 js-select-simple select--no-search">
                                <select name="event">
                                    <option disabled="disabled" selected>Event</option>
                                    <option>Freshers</option>
                                    <option>Fres</option>
                                    <%= op %>
                                    <!-- <option>Freshers</option>
                                    <option>Farewell</option>
                                    <option>Roadshow</option> -->
                                </select>
                                
                                <div class="select-dropdown"></div>
                            </div>
                        </div>
                        <div class="input-group">
                            <div class="rs-select2 js-select-simple select--no-search">
                                <select name="orientation">
                                    <option disabled="disabled" selected="selected">Orientaiton</option>
                                    <option>Potrait</option>
                                    <option>Landscape</option> 
                                </select>
                                
                                <div class="select-dropdown"></div>
                            </div>
                        </div>
                        <div class="p-t-10">
                            <button class="btn btn--pill btn--green" type="submit" style="margin:0px 100px">Submit</button>
                        </div>
                    </form>
                </div>
                <div class="form-2">
                    <form action="/upload7054" method="POST" >
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="name" name="name">
                        </div>
                        <div class="input-group">
                            <textarea class="input--style-3" rows=3 cols="37" name="dis" placeholder="Description"></textarea>
                           
                        </div>
                       
                        
                        <div class="p-t-10 submitbox">
                            <button class="btn btn--pill btn--green" type="submit" style="margin:0px 100px">Submit</button>
                        </div>
                    </form>
                </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <!-- Vendor JS-->
    <script src="vendor/select2/select2.min.js"></script>
    <script src="vendor/datepicker/moment.min.js"></script>
    <script src="vendor/datepicker/daterangepicker.js"></script>

    <!-- Main JS-->
    <script src="js/global.js"></script>

</body><!-- This templates was made by Colorlib (https://colorlib.com) -->

</html>
